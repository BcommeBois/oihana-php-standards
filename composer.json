{
    "name" : "oihana/php-standards" ,
    "type" : "library" ,
    "description" : "The Oihana PHP Standards library" ,
    "keywords" : [ "php" , "iso" , "uncefact" , "unm49" ] ,
    "homepage" : "https://github.com/BcommeBois/oihana-php-schema" ,
    "license" : "MPL-2.0" ,
    "authors" : [
        {
            "name" : "Marc Alcaraz" ,
            "email" : "marc@ooop.fr" ,
            "homepage" : "https://www.ooop.fr" ,
            "role" : "Project Founder, Lead Developer."
        }
    ],
    "support": {
        "email": "marc@ooop.fr"
    },
    "autoload" :
    {
        "psr-4" :
        {
            "org\\iso\\": "src/org/iso",
            "org\\unece\\uncefact\\": "src/org/unece/uncefact",
            "org\\unstats\\": "src/org/unstats"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "org\\unece\\uncefact\\": "tests/org/unece/uncefact"
        }
    },
    "require": {
        "php": ">=8.4",
        "oihana/php-core": "dev-main"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "nunomaduro/collision": "^8.8"
    }
}
